# KONG SETTING
KONG_DB_NAME=kong
KONG_DB_USERNAME=user
KONG_DB_PASSWORD=secret
KONG_DB_HOST=postgesd
KONG_DB_PORT=5432

KONG_DATABASE=postgres
KONG_PROXY_ACCESS_LOG=/dev/stdout
KONG_ADMIN_ACCESS_LOG=/dev/stdout
KONG_PROXY_ERROR_LOG=/dev/stderr
KONG_ADMIN_ERROR_LOG=/dev/stderr
KONG_ADMIN_LISTEN=0.0.0.0:8001, 0.0.0.0:8444 ssl

KONG_PROXY_PORT=8001
KONG_PROXY_SSL_PORT=8443

# KONGA SETTING
KONGA_DB_NAME=konga
KONGA_DB_USERNAME=user
KONGA_DB_PASSWORD=secret
KONGA_DB_HOST=postgresd
KONGA_DB_PORT=5432

KONGA_TOKEN_SECRET=some-secret-token
KONGA_ENV=production
KONGA_PORT=9001
